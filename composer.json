{
  "name": "wyrihaximus/react-http-middleware-metrics",
  "description": "react/http middleware that compresses JS/CSS response bodies",
  "license": "MIT",
  "authors": [
    {
      "name": "Cees-Jan Kiewiet",
      "email": "ceesjank@gmail.com"
    }
  ],
  "require": {
    "php": "^8",
    "psr/http-message": "^1.0.1",
    "react/http": "^1.5.0",
    "react/promise": "^3 || ^2.8",
    "wyrihaximus/constants": "^1",
    "wyrihaximus/metrics": "^1"
  },
  "require-dev": {
    "wyrihaximus/async-test-utilities": "^5.0.12"
  },
  "autoload": {
    "psr-4": {
      "WyriHaximus\\React\\Http\\Middleware\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WyriHaximus\\React\\Tests\\Http\\Middleware\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "infection/extension-installer": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "ergebnis/composer-normalize": true
    },
    "platform": {
      "php": "8.0.99"
    },
    "sort-packages": true
  },
  "scripts": {
    "post-install-cmd": [
      "composer normalize"
    ],
    "post-update-cmd": [
      "composer normalize"
    ]
  }
}
